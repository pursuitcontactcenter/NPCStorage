    <validationRules>
        <fullName>As_Needed_for_Type_and_Schedule_Type</fullName>
        <active>true</active>
        <description>If As Needed is selected as Type, then Schedule Type must match.</description>
        <errorConditionFormula>IF(ISPICKVAL( Type , &quot;As Needed&quot;), 
NOT(ISPICKVAL( ScheduleType , &quot;As Needed&quot;)),
FALSE
)</errorConditionFormula>
        <errorDisplayField>Type</errorDisplayField>
        <errorMessage>A Position with a Type of &quot;As Needed&quot; must have a Schedule Type of &quot;As Needed&quot;</errorMessage>
    </validationRules>
