        <description>Calls a flow that generates a benefit disbursement record and creates any missing related records required for program enrollment and benefit assignment. Sets the benefit, disbursement quantity, and participant to the values selected in the ScreenCollectBenefitDisbursementDetails screen. Stores the resulting collection in the Outputs from CreateParticipantBenefits record collection.</description>
        <name>CreateParticipantBenefits</name>
        <label>Create Participant Benefits</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <connector>
            <targetReference>HaveParticipantBenefits</targetReference>
        </connector>
        <flowName>PgmMgmt__CreateParticipantBenefits</flowName>
        <inputAssignments>
            <name>benefitId</name>
            <value>
                <elementReference>BenefitLookup.selectedChoiceValues</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <name>disbursementDate</name>
            <value>
                <elementReference>DisbursementDate</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <name>disbursementQuantity</name>
            <value>
                <elementReference>DisbursementQuantity</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <name>participantId</name>
            <value>
                <elementReference>ParticipantLookup.selectedChoiceValues</elementReference>
            </value>
        </inputAssignments>
        <storeOutputAutomatically>true</storeOutputAutomatically>
    </subflows>
    <variables>
        <description>Stores the account ID for the participant of a benefit disbursement.</description>
        <name>ParticipantId</name>
        <dataType>String</dataType>
        <isCollection>false</isCollection>
        <isInput>false</isInput>
        <isOutput>false</isOutput>
    </variables>
    <variables>
        <description>Stores the ID for the account passed into the flow.</description>
        <name>recordId</name>
        <dataType>String</dataType>
        <isCollection>false</isCollection>
        <isInput>true</isInput>
        <isOutput>false</isOutput>
    </variables>
</Flow>
