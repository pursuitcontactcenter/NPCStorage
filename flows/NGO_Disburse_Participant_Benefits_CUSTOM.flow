    <decisions>
        <description>Determines whether the CreateParticipantBenefits flow ran and created records by checking if the CreateParticipantBenefits flow didn’t return a fault message and if the benefitDisbursementId contains a value.</description>
        <name>HaveParticipantBenefits</name>
        <label>Have Participant Benefits?</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnector>
            <targetReference>ScreenDisplayErrorCreateParticipantBenefit</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>No (Default)</defaultConnectorLabel>
        <rules>
            <name>HasParticipantBenefits</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>CreateParticipantBenefits.benefitDisbursementId</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>CreateParticipantBenefits.benefitDisbursementId</leftValueReference>
                <operator>IsBlank</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Flow.FaultMessage</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Flow.FaultMessage</leftValueReference>
                <operator>IsBlank</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>ScreenDisplayConfirmationBenefitDisbursement</targetReference>
            </connector>
            <label>Yes</label>
        </rules>
    </decisions>
    <decisions>
        <description>Determines whether the CreateAnotherBenefitDisbursementCheckbox on the ScreenDisplayConfirmationBenefitDisbursement screen is set to true.</description>
        <name>IsCreateAnotherBenefitDisbursementSelected</name>
        <label>Is Create Another Benefit Disbursement Selected?</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnectorLabel>No (Default)</defaultConnectorLabel>
        <rules>
            <name>ShouldCreateBenefitDisbursement</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>CreateAnotherBenefitDisbursementCheckbox</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <isGoTo>true</isGoTo>
                <targetReference>ScreenCollectBenefitDisbursementDetails</targetReference>
            </connector>
            <label>Yes</label>
        </rules>
    </decisions>
